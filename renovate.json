{
  "rangeStrategy": "pin",
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "matchPackagePatterns": [".*"],
      "matchUpdateTypes": ["minor"],
      "automerge": true
    },
    {
      "matchPackagePatterns": [".*"],
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": ["minor"],
      "automerge": true
    }
  ]
}
